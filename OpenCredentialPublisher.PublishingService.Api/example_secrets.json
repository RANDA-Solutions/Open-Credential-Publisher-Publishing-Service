{
  "AppBaseUri": "https://apihostgoeshere", // "https://localhost:5001" for debugging
  "ApplicationDbConnectionString": "{Sql Connection String}",
  "IdentityDbConnectionString": "{Sql Connection String}",
  "AccessKeyUrl": "https://(web wallet url goes here)/credentials/connect",
  "AzureKeyVault": {
    "AzureAppClientId": "{Azure AD ClientID}",
    "AzureAppClientSecret": "{Azure AD ClientSecret}",
    "KeyVaultBaseUri": "https://{yourvaultname}.vault.azure.net/",
    "CertificateName": "ocp-service-local-signing-cert",
    "AzureTenantId": "tenantid"
  },

  "AzureQueue": {
    "StorageConnectionString": "DefaultEndpointsProtocol=https;AccountName={accountname};AccountKey={key};EndpointSuffix=core.windows.net"
  },

  "AzureBlob": {
    "StorageConnectionString": "DefaultEndpointsProtocol=https;AccountName={accountname};AccountKey={key};EndpointSuffix=core.windows.net"
  },

  "APPINSIGHTS_INSTRUMENTATIONKEY": "{Instrumentation Key}"
}